{
  "name": "mao-lang",
  "displayName": "mao-lang",
  "description": "Extension for syntax highlighting in Mao Programming Language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "mao",
      "aliases": ["Mao", "Mao"],
      "extensions": [".ma",".mao",".mo",".m"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "Mao",
      "scopeName": "source.mao",
      "path": "./syntaxes/mao.tmLanguage.json"
    }]
  }
}
